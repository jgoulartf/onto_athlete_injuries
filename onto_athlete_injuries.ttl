###  http://example.com#Atleta
:Atleta rdf:type owl:Class ;
        rdfs:subClassOf gufo:FunctionalComplex ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :faz ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Exercicio
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty gufo:mediates ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Historico_lesoes
                        ] ;
        owl:disjointWith :Profissional_de_saude .


